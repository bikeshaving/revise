{
  "name": "@brainkim/shredder",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf ./lib",
    "prebuild": "yarn clean",
    "build": "tsc",
    "prebench": "yarn build",
    "bench": "NODE_ENV=production node ./bench",
    "develop": "yarn test --watch",
    "test": "jest",
    "lint": "eslint --ext js,ts src bench",
    "publish-lib": "yarn clean && yarn lint && yarn test && shx grep -v \"\\\"private\\\": true,\" package.json > lib/package.json && shx cp yarn.lock README.md ./lib && cd ./lib && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.0.0",
    "@typescript-eslint/parser": "^1.0.0",
    "benchmark": "^2.1.4",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fast-check": "^1.10.0",
    "jest": "^23.6.0",
    "microtime": "^2.1.8",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
