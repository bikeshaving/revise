{
  "name": "@b9g/revise",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./edit.js": {
      "import": "./dist/edit.js",
      "require": "./dist/edit.cjs"
    },
    "./contentarea.js": {
      "import": "./dist/contentarea.js",
      "require": "./dist/contentarea.cjs"
    },
    "./keyer.js": {
      "import": "./dist/keyer.js",
      "require": "./dist/keyer.cjs"
    },
    "./history.js": {
      "import": "./dist/history.js",
      "require": "./dist/history.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.js",
    "clean": "shx rm -rf dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "preshipit": "npm run typecheck && npm run test && npm run build",
    "shipit": "cd dist && npm publish",
    "test": "playwright-test --runner uvu",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "magic-string": "^0.26.2",
    "playwright-test": "^8.1.1",
    "prettier": "^2.7.1",
    "rollup": "^2.78.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
