{
  "name": "@brainkim/shredder",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -rf ./lib",
    "prebuild": "yarn clean",
    "build": "tsc",
    "prebench": "yarn build",
    "bench": "NODE_ENV=production node ./bench",
    "develop": "yarn test --watch",
    "test": "jest",
    "lint": "eslint --ext js,ts src bench",
    "publish-lib": "yarn clean && yarn lint && yarn test && shx grep -v \"\\\"private\\\": true,\" package.json > lib/package.json && shx cp yarn.lock README.md ./lib && cd ./lib && npm publish"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.6",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "benchmark": "^2.1.4",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fast-check": "^1.10.1",
    "jest": "^24.1.0",
    "microtime": "^3.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3"
  }
}
